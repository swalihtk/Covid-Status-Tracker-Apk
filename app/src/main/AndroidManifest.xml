<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coviddailyupdates">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/corona_world"
        android:label="@string/app_name"
        android:roundIcon="@drawable/corona_world"
        android:supportsRtl="true"
        android:theme="@style/Theme.CovidDailyUpdates">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Corona_State"
            android:label="Corona State Wise Updates"
            android:parentActivityName=".IndiaCorona" />
        <activity android:name=".AboutUs" />
        <activity
            android:name=".IndiaCorona"
            android:label="India Corona Updates"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WorldCorona"
            android:label="World Corona Updates"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>